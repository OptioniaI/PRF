            game:GetService("RunService").Heartbeat:connect(function()
                        v.Velocity = Vector3.new(0,35,0)
                        wait(0.5)
                        end)
                    end
                end
            end)()
            task.wait()
            local plr = game.Players.LocalPlayer
            game:GetService("RunService").Stepped:Connect(function()
               setsimulationradius(9e9,9e9)
               plr.ReplicationFocus = workspace
               settings().Physics.AllowSleep = false
            end)

            local PlrHRP = game.Players.LocalPlayer.Character.HumanoidRootPart

            local LArm = game.Players.LocalPlayer.Character["Left Arm"]
            local LLeg = game.Players.LocalPlayer.Character["Left Leg"]
            local RLeg = game.Players.LocalPlayer.Character["Right Leg"]

            LArm:BreakJoints()
            LLeg:BreakJoints()
            RLeg:BreakJoints()

