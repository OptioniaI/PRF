prefix = "."
local LPlayer = game.Players.LocalPlayer
LPlayer.Chatted:Connect(function(msg)
    msg = msg:lower()
    if string.sub(msg,1,3) == "/e " then
    	msg = string.sub(msg,4)
    end
    if string.sub(msg,1,1) == prefix then
    	local cmd
    	local space = string.find(msg," ")
    	if space then
    		cmd = string.sub(msg,2,space-1)
    	else
    		cmd = string.sub(msg,2)
    	end
    
        if cmd == "sit" then
            -- makes u sit
            local Plr = game.Players.LocalPlayer
	        local Char = Plr.Character
            local Humanoid = Char.Humanoid

            Humanoid.Sit = true
        end
        if cmd == "amp" then
            local amplify = string.sub(msg,space+1)
            _G.AMP = amplify
        end
        if cmd == "toolhat" or cmd ==  "th" then
            local plr = game.Players.LocalPlayer
            local backpack = plr.Backpack
            local plrtool = backpack:FindFirstChildOfClass("Tool")
            local char = plr.Character
            
            plrtool.Parent = char
            
            coroutine.wrap(function()
            for i,v in next, game:GetService("Players").LocalPlayer.Character:GetDescendants() do
            
            if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
                
            game:GetService("RunService").Heartbeat:connect(function()
                        v.Velocity = Vector3.new(0,35,0)
                        wait(0.5)
                        end)
                    end
                end
            end)()
            
            local plr = game.Players.LocalPlayer
            local char = plr.Character
            local arm = char["Right Arm"]
            local tool = char:FindFirstChildOfClass("Tool")
            local height = 1.25
            local delay = 0
            
            arm:FindFirstChildOfClass("Weld"):Destroy()
            
            BV = Instance.new("BodyVelocity", tool.Handle)
            BAV = Instance.new("BodyAngularVelocity", tool.Handle)
            ATC = Instance.new("Attachment", tool.Handle)
            ATC2 = Instance.new("Attachment", char.Head)
            ALIGN = Instance.new("AlignPosition", tool.Handle)
            
            BV.Name = "BV"
            BAV.Name = "BAV"
            ALIGN.Name = "ALIGN"
            ATC.Name = "ATC"
            ATC2.Name = "ATC2"
            BV.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            BAV.AngularVelocity = Vector3.new(0, 180, 0)
            BAV.MaxTorque = Vector3.new(300000, 300000, 300000)
            ALIGN.ApplyAtCenterOfMass = true
            ALIGN.RigidityEnabled = false
            ALIGN.MaxVelocity = math.huge
            ALIGN.MaxForce = 300000
            ALIGN.Responsiveness = 200
            ALIGN.Attachment0 = ATC
            ALIGN.Attachment1 = ATC2
            
            tool.Handle:FindFirstChildOfClass("TouchTransmitter"):Destroy()
            
            while task.wait() do
            	tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, 0.99993068, 2.29192985e-08, -0.0117744831, -2.30553692e-08, 1, -1.14206102e-08, 0.0117744831, 1.16912835e-08, 0.99993068)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, 0.568915665, 6.88560431e-10, 0.822395861, -7.1158629e-10, 1, -3.45001583e-10, -0.822395861, -3.88928806e-10, 0.568915665)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, 0.0466643982, -3.97823428e-08, 0.998910606, 2.27359074e-08, 1, 3.87636128e-08, -0.998910606, 2.09022595e-08, 0.0466643982)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, -0.723272383, 6.34869179e-09, 0.690562904, -2.79302999e-08, 1, -3.84467604e-08, -0.690562904, -4.70951065e-08, -0.723272383)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, -0.999279797, 2.85674684e-09, 0.037946146, 3.49064666e-09, 1, 1.66389942e-08, -0.037946146, 1.67594667e-08, -0.999279797)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, -0.733429313, -2.16268226e-09, -0.679765761, 3.47685325e-09, 1, -6.93284141e-09, 0.679765761, -7.44819495e-09, -0.733429313)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, -0.0117749879, 2.54436685e-08, -0.99993068, -4.14866257e-08, 1, 2.59339714e-08, 0.99993068, 4.17891215e-08, -0.0117749879)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, 0.666862369, -1.36569234e-09, -0.745180905, -4.58820454e-10, 1, -2.24329755e-09, 0.745180905, 1.83787496e-09, 0.666862369)
                task.wait(delay)
                tool.Handle.CFrame = char.Head.CFrame * CFrame.new(0, height, 0, 0.99993068, 2.29800392e-08, -0.0117746368, -2.40143212e-08, 1, -8.76984174e-08, 0.0117746368, 8.79751028e-08, 0.99993068)
            end
        end
        if cmd == "clicktp" or cmd == "ctp" or cmd == "tweenctp" or cmd == "twclicktp" then
            local speed = 50
            _G.ClickTP = true

            local Imput = game:GetService("UserInputService")
            local Plr = game.Players.LocalPlayer
            local Root = game.Players.LocalPlayer.Character.Humanoid.RootPart
            local Mouse = Plr:GetMouse()

            function To(position)
            local Chr = Plr.Character
            if Chr ~= nil then
            if _G.ClickTP then
            	local ts = game:GetService("TweenService")
            	local char = game.Players.LocalPlayer.Character
            	local hm = char.HumanoidRootPart
            	local dist = (hm.Position - Mouse.Hit.p).magnitude
            	local tweenspeed = dist/tonumber(speed)
            	local ti = TweenInfo.new(tonumber(tweenspeed), Enum.EasingStyle.Linear)
            	local tp = {CFrame = CFrame.new(position)}
            	ts:Create(hm, ti, tp):Play()
            end
            	end
            end

            Imput.InputBegan:Connect(function(input)
               if input.UserInputType == Enum.UserInputType.MouseButton1 then
                   To(Mouse.Hit.p)
               end
            end)
        end
        if cmd == "unclicktp" or cmd == "unctp" then
            _G.ClickTP = false
        end
        if cmd == "pref" or cmd == "prefix" or cmd == "pf" then
            -- change prefix cmd
            local prefixchanger = string.sub(msg,space+1)
            prefix = prefixchanger
        end
        if cmd == "hidesword" or cmd == "hss" then
            Plr.Backpack:FindFirstChildOfClass("Tool").GripPos = Vector3.new(1500, 0, 0)
            Plr.Backpack:FindFirstChildOfClass("Tool").Parent = Plr.Character
            --StopAnimForTool
            task.wait(0.15)
	        local Char = Plr.Character
	        local Hum = Char:FindFirstChildOfClass("Humanoid") or Char:FindFirstChildOfClass("AnimationController")

	        for i,v in next, Hum:GetPlayingAnimationTracks() do
	        	v:Stop()
	        end
        end
        if cmd == "shortp" then
            coroutine.wrap(function()
                for i,v in next, game:GetService("Players").LocalPlayer.Character:GetDescendants() do

            if v:IsA("BasePart") and v.Name ~="HumanoidRootPart" then
    
            game:GetService("RunService").Heartbeat:connect(function()
                        v.Velocity = Vector3.new(0,35,0)
                        wait(0.5)
                        end)
                    end
                end
            end)()
            task.wait()
            local plr = game.Players.LocalPlayer
            game:GetService("RunService").Stepped:Connect(function()
               setsimulationradius(9e9,9e9)
               plr.ReplicationFocus = workspace
               settings().Physics.AllowSleep = false
            end)
    
            local runservice=game:service"RunService";
            local player=game:service"Players"["LocalPlayer"];
            local character=player["Character"];
            local blacklisted="Head Torso HumanoidRootPart Right Arm Left Arm";
            local limbs={};
    
            for i,v in next,character:children() do
                if (v.ClassName=="Part") and not blacklisted:find(v.Name) then
                    v:BreakJoints();
                    limbs[v.Name]=v;
                end
            end
    
            while runservice["Heartbeat"]:Wait() do
                limbs["Left Leg"].CFrame=character["HumanoidRootPart"].CFrame*CFrame.new(0.03425, -1.01, -1.75, 0.994062066, 0.0959605724, -0.0513048135, 0.0515425913, 8.88251805e-07, 0.998670816, 0.0958330631, -0.99538511, -0.00494517339);
                limbs["Right Leg"].CFrame=character["HumanoidRootPart"].CFrame*CFrame.new(0.01, -1, -1.5, 0.994062066, 0.0959605724, -0.0513048135, 0.0515425913, 8.88251805e-07, 0.998670816, 0.0958330631, -0.99538511, -0.00494517339);
            end
        end
        if cmd == "longp" then
            coroutine.wrap(function()
            for i,v in next, game:GetService("Players").LocalPlayer.Character:GetDescendants() do

            if v:IsA("BasePart") and v.Name ~="HumanoidRootPart" then
    
            game:GetService("RunService").Heartbeat:connect(function()
                        v.Velocity = Vector3.new(0,35,0)
                        wait(0.5)
                        end)
                    end
                end
            end)()
            task.wait()
            local plr = game.Players.LocalPlayer
            game:GetService("RunService").Stepped:Connect(function()
               setsimulationradius(9e9,9e9)
