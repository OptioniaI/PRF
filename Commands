            end
            local function tp(player,player2)
            local char1,char2=player.Character,player2.Character
            if char1 and char2 then
            char1.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
            end
            end
            local function getout(player,player2)
            local char1,char2=player.Character,player2.Character
            if char1 and char2 then
            char1:MoveTo(char2.Head.Position)
            end
            end
            tp(tpps, game.Players.LocalPlayer)
            wait(0.1)
            tp(tpps, game.Players.LocalPlayer)
            wait(0.3)
            getout(game.Players.LocalPlayer, tpps)
            wait(0.2)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = NOW
        end
        if cmd == "re" or cmd == "refresh" then
            local plr = game.Players.LocalPlayer
            local hrp = plr.Character.HumanoidRootPart
            local pose = hrp.CFrame
            local char = plr.Character

            char:BreakJoints()
            char.Humanoid.Health = 0
            task.wait(0.09)
            char:BreakJoints()
            char.Humanoid.Health = 0
            char.Head:Destroy()
            char.Torso:Destroy()
            char.HumanoidRootPart:Destroy()
            char["Left Arm"]:Destroy()
            char["Left Leg"]:Destroy()
            char["Right Arm"]:Destroy()
            char["Right Leg"]:Destroy()
            char:FindFirstChildOfClass('Humanoid'):Destroy()
            task.wait()
            plr.CharacterAdded:Wait():WaitForChild("HumanoidRootPart").CFrame = pose
    	end
        if cmd == "sh" or cmd == "hop" or cmd == "serverhop" then
    	    local h = {}
    	    for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
    		    if type(v) == "table" and v.maxPlayers > v.playing and v.id ~= game.JobId then
    			    h[#h + 1] = v.id
    		    end
    	    end
    	    if #h > 0 then
    		    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, h[math.random(1, #h)])
    	    else
    		    warn("Couldn't find a server, still looking..")
    	    end
    	end
