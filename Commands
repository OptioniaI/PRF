            local speed = 55
            
            local Bind = false
            local UIS = game:GetService("UserInputService")
            BV.MaxForce = Vector3.new(0,0,0)
            UIS.InputBegan:Connect(function(input,wrk)
            if wrk then return end
            	if input.KeyCode == Enum.KeyCode.Minus and not Bind then
            		Bind = true
            		BV.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
            	elseif input.KeyCode == Enum.KeyCode.Minus and Bind then
            		Bind = false
            		BV.MaxForce = Vector3.new()
            	end
            end)
            mouse.Move:Connect(function() 
            	BV.Velocity = CFrame.new(character.Position,mouse.Hit.Position).LookVector * speed
            end)
