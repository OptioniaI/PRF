            local CFrameEnd = TargetPos
            local OPos = Plr.CFrame
            local Time = 0
            local tween =  game:GetService("TweenService"):Create(Plr, TweenInfo.new(Time), {CFrame = CFrameEnd * CFrame.new(-1.5, 0, 1.15)})

            Tool.Parent = game.Players.LocalPlayer.Character
            tween:Play()
            task.wait(0.110)
            Plr.CFrame = OPos
        end
        if cmd == "tgo" or cmd == "tweenrgo" or cmd == "tweenrg" then
            local shorted = string.sub(msg,space+1)
            local plr = game.Players.LocalPlayer
            local char = plr.Character

            char:BreakJoints()
            char.Humanoid.Health = 0
            task.wait(0.09)
            char:BreakJoints()
            char.Humanoid.Health = 0
            char.Head:Destroy()
            char.Torso:Destroy()
            char.HumanoidRootPart:Destroy()
            char["Left Arm"]:Destroy()
            char["Left Leg"]:Destroy()
            char["Right Arm"]:Destroy()
            char["Right Leg"]:Destroy()
            char:FindFirstChildOfClass('Humanoid'):Destroy()
            task.wait(0.095)
            local Plr = game.Players.LocalPlayer.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
            local Tool = game.Players.LocalPlayer.Backpack:FindFirstChildOfClass('Tool')
            local Target = GetPlayer(shorted)
            local TargetPos = Target.Character.HumanoidRootPart.CFrame
            local CFrameEnd = TargetPos
            local speed = 50
            local dist = (Plr.Position - Target.Character.HumanoidRootPart.Position).magnitude
            local tweenspeed = dist/tonumber(speed)
            local TI = TweenInfo.new(tonumber(tweenspeed), Enum.EasingStyle.Linear)
            local TP = {CFrame = CFrameEnd * CFrame.new(-1.5, 0, 1.5)}
            
            Tool.Parent = game.Players.LocalPlayer.Character
            TS:Create(Plr, TI, TP):Play()
        end 
        if cmd == "tpk" or cmd == "tweentk" then
            local var = string.sub(msg,space+1)
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            local chhr =  game.Players.LocalPlayer.Character
            local them = GetPlayer(var)

            spawn(function()
                _G.EE = true
                while _G.EE == true do
                task.wait(0.065)
                plr.CFrame = them.Character.HumanoidRootPart.CFrame * CFrame.new(-1.5, 0, 1)
                task.wait(0.125)
                chhr:BreakJoints()
                _G.EE = false
                end
            end)
        end
        if cmd == "k" or cmd == "kill" then
            local var = string.sub(msg,space+1)
            local plr = game.Players.LocalPlayer.Character.HumanoidRootPart
            local op = plr.CFrame
            local them = GetPlayer(var)

            spawn(function()
                _G.EE = true
                while _G.EE == true do
                task.wait(0.065)
                plr.CFrame = them.Character.HumanoidRootPart.CFrame * CFrame.new(-1.5, 0, 1)
                task.wait(0.065)
                plr.CFrame = op
                task.wait()
                _G.EE = false
                end
            end)
        end

    end
end)

function GetPlayer(String)
    local plr
    local strl = String:lower()
        for i, v in pairs(game:GetService("Players"):GetPlayers()) do
            if v.Name:lower():sub(1, #String) == String:lower() then
                plr = v
            end 
        end
        if String == "me" then
                plr = game.Players.LocalPlayer
            end
        if String == "" or String == " " then
           plr = nil
        end
    return plr
end
